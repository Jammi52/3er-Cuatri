Algoritmo Areayperimetrodeltriangulo
		Escribir " Dame el valor de la altura:";
		Leer altura;
		Escribir "Dame el valor de la base:";
		Leer base;
		area <- base*altura;
		perimetro <- base+altura+RC(base*base+altura*altura);
		Escribir "Valor de area: ", area;
		Escribir "Valor de perimetro: ", perimetro;

FinAlgoritmo
